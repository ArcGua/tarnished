{
  "extends": "@antfu",
  "rules": {
    "curly": "off",
    "consistent-return": "off",
    "no-empty": "off",
    "@typescript-eslint/no-unused-vars": [
      "error",
      {
        "vars": "local",
        "args": "none",
        "ignoreRestSiblings": true,
        "caughtErrors": "none"
      }
    ]
  }
}